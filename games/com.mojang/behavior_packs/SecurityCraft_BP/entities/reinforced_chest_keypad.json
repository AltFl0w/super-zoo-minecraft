{
  "format_version": "1.19.0",
  "minecraft:entity": {
    "description": {
      "identifier": "scb:reinforced_chest_keypad",
      "is_spawnable": false,
      "is_summonable": true
    },
    "component_groups": {
      "scb:despawn": {
        "minecraft:instant_despawn": {}
      },
      "scb:add_inventory": {
        "minecraft:inventory": {
          "container_type": "container",
          "inventory_size": 27,
          "private": false,
          "restrict_to_owner": false
        }
      },
      "scb:remove_inventory": {
        "minecraft:inventory": {
          "container_type": "container",
          "inventory_size": 27,
          "private": true,
          "restrict_to_owner": false
        }
      }
    },
    "components": {
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "all",
            "deals_damage": false
          }
        ]
      },
      "minecraft:loot": {
        "table": "loot_tables/reinforced_chest_keypad.json"
      },
      "minecraft:interact": {
        "interactions": [
          {
            "play_sounds": "block.stonecutter.use",
            "swing": true,
            "on_interact": {
              "filters": {
                "all_of": [
                  {
                    "test": "has_equipment",
                    "subject": "other",
                    "value": "scb:remover"
                  },
                  {
                    "test": "has_tag",
                    "subject": "self",
                    "value": "scb:locked",
                    "operator": "!="
                  }
                ]
              },
              "event": "scb:remove"
            }
          },
          {
            "on_interact": {
              "filters": {
                "test": "has_tag",
                "subject": "self",
                "value": "scb:locked"
              }
            }
          }
        ]
      },
      "minecraft:health": {
        "value": 1
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:knockback_resistance": {
        "value": 1
      },
      "minecraft:movement": {
        "value": 0
      },
      "minecraft:collision_box": {
        "height": 1,
        "width": 1
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": false
      }
    },
    "events": {
      "scb:add_inventory": {
        "remove": {
          "component_groups": ["scb:remove_inventory"]
        },
        "add": {
          "component_groups": ["scb:add_inventory"]
        }
      },
      "scb:remove_inventory": {
        "remove": {
          "component_groups": ["scb:add_inventory"]
        },
        "add": {
          "component_groups": ["scb:remove_inventory"]
        }
      },
      "scb:remove": {
        "queue_command": {
          "command": ["kill @s", "setblock ~ ~ ~ air"]
        }
      },
      "scb:despawn": {
        "add": {
          "component_groups": ["scb:despawn"]
        }
      },
      "minecraft:entity_born": {
        "add": {
          "component_groups": ["tameable"]
        }
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["tameable"]
        }
      },
      "tamed": {
        "add": {
          "component_groups": ["tamed"]
        }
      }
    }
  }
}
