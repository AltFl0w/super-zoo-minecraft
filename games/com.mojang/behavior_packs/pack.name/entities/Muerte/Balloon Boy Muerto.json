{
	"format_version": "1.8.0",
	"minecraft:entity": {
		"description": {
			"identifier": "df:balloon_boy_m",
			"is_spawnable": false,
			"is_summonable": false,
            "is_experimental": false,
            "scripts": {
                "animate": [
                    "no_showtime"
                ]
            },
            "animations": {
                "no_showtime": "controller.animation.df_animatronicos.no_showtime"
			}
		},
		"component_groups": {
            "minecraft:dead_": {
                "minecraft:collision_box": {
                    "width": 0.0,
                    "height": 0.0
                },
                "minecraft:timer": {
                    "looping": false,
                    "randomInterval": false,
                    "time": 120,
                    "time_down_event": {
                        "event": "minecraft:event_dead"
                    }
                }
            },
            "minecraft:despawndead": {
                "minecraft:type_family": {
                    "family": ["despawn_"]
                }
            },
            "minecraft:behavior.float": {
                "priority": 0
            }
        },
        "components": {
            "minecraft:jump.static": {},
         			"minecraft:scale": {
        				"value": 0.6
        			},
            "minecraft:knockback_resistance": {
                "value": 1
        			},
            "minecraft:despawn": {
                "filters": {
                    "all_of": [{
                            "test": "is_family",
                            "subject": "self",
                            "value": "despawn_"
                        }
                    ]
                }
            },
            "minecraft:push_through": {
                "value": 1
            },
            "minecraft:fire_immune": true,
            "minecraft:health": {
                "value": {
                    "range_min": 10,
                    "range_max": 10
                }
            },
			"minecraft:damage_sensor": [{
                    "deals_damage": false
                }
            ],
            "minecraft:physics": {
                "has_gravity": true,
                "has_collusion": false
            }
        },
        "events": {
            "minecraft:entity_transformed": {
                "add": {
                    "component_groups": ["minecraft:dead_"]
                }
            },
            "minecraft:event_dead": {
                "add": {
                    "component_groups": ["minecraft:despawndead"]
                }
            }
        }
    }
}
