        "scale": "0.9375",
        "initialize": [
          "variable.is_holding_right = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;"
        ],
        "pre_animation": [
		  "variable.symbiote_rage_mode = q.property('arathnido:suit_ability3') == 3 && q.equipped_item_any_tag('slot.armor.legs', 'spiderman_symbiote_powers');",
		  "variable.item.off_hand = q.equipped_item_any_tag('slot.weapon.offhand', 'item_off_hand');",
		  "variable.off_spiderman_combat = q.property('arathnido:suit_ability3') == 3 && q.equipped_item_any_tag('slot.armor.legs', 'spiderman_symbiote_powers');",
		  "variable.spiderman_off_falling = q.property('arathnido:animation') == 11 || q.property('arathnido:attacks') == 5 || q.property('arathnido:attacks') == 6;",
		  "variable.itemSpecial = q.equipped_item_any_tag('slot.weapon.offhand', 'captan_america_item') || q.equipped_item_any_tag('slot.weapon.mainhand', 'captan_america_item') || q.equipped_item_any_tag('slot.weapon.maindhand', 'gun');",
		  "variable.escudo_offhand = q.equipped_item_any_tag('slot.weapon.offhand', 'captan_america_item') && q.is_sneaking && q.is_on_ground;",
		  "variable.off_move_hands = q.equipped_item_any_tag('slot.weapon.mainhand', 'off_move_hands');",
		  "variable.off_walk_on_fly = (q.equipped_item_any_tag('slot.armor.head', 'off_walk_on_fly') || q.equipped_item_any_tag('slot.armor.feet', 'off_walk_on_fly') || q.equipped_item_any_tag('slot.armor.legs', 'off_walk_on_fly') || q.equipped_item_any_tag('slot.armor.chest', 'off_walk_on_fly')) && !q.is_on_ground;",
		  "variable.gun_hand = q.equipped_item_any_tag('slot.weapon.mainhand', 'gun') || q.equipped_item_any_tag('slot.weapon.mainhand', 'gwenpool_micro_uzi') || q.equipped_item_any_tag('slot.weapon.mainhand', 'gwenpool_rocket_launcher') || q.equipped_item_any_tag('slot.weapon.mainhand', 'deadpool_pistols');",
		  "variable.off_hands = q.equipped_item_any_tag('slot.armor.head', 'off_hands');",
		  "variable.custom_attack = q.equipped_item_any_tag('slot.armor.head', 'special_skin') || q.equipped_item_any_tag('slot.weapon.mainhand', 'custom_attack');",
		  "variable.custom_look = q.property('mzaddon:balanceo') == true || q.property('mzaddon:caida_picada') == true || q.property('spiderman:poderes') == true;",
		  "variable.off_shield = q.equipped_item_any_tag('slot.weapon.mainhand', 'off_shield');",
		  "variable.off_riding = q.property('arathnido:custom_riding') == 0;",
		  "variable.item_using = q.equipped_item_any_tag('slot.weapon.mainhand', 'item_using') || q.equipped_item_any_tag('slot.weapon.mainhand', 'new_gun');",
		  "variable.m32 = q.equipped_item_any_tag('slot.weapon.mainhand', 'm32_render') || q.equipped_item_any_tag('slot.weapon.mainhand', 'bucky_grenade_launcher_render');",
		  "variable.two_hands_suit = variable.is_first_person && (q.equipped_item_any_tag('slot.weapon.mainhand', 'new_gun') || q.equipped_item_any_tag('slot.weapon.mainhand', 'item_hand') || q.get_equipped_item_name(0, 1) == '');",
		  "variable.two_hands = q.equipped_item_any_tag('slot.weapon.mainhand', 'new_gun') || q.equipped_item_any_tag('slot.weapon.mainhand', 'item_hand');",
		  "variable.off_second_hand = !variable.escudo_offhand && !q.equipped_item_any_tag('slot.weapon.mainhand', 'new_gun') && (q.equipped_item_any_tag('slot.weapon.mainhand', 'off_second_hand') || q.get_equipped_item_name(0, 1) == '');",
		  "variable.special_skin = (!q.equipped_item_any_tag('slot.armor.head', 'convert_entity') && !q.equipped_item_any_tag('slot.armor.feet', 'suit') && !q.equipped_item_any_tag('slot.armor.feet', 'special_suit'));",
		  "variable.sneaking_off = (q.equipped_item_any_tag('slot.armor.feet', 'moon_knight_suit') && q.is_sneaking) || (q.equipped_item_any_tag('slot.armor.feet', 'ironman_suit') && (!q.is_on_ground || q.equipped_item_any_tag('slot.weapon.mainhand', 'ironman_shield')));",
		  "variable.custom_move = !q.equipped_item_any_tag('slot.armor.head', 'special_skin') && !q.equipped_item_any_tag('slot.armor.feet', 'ironman_suit') && !q.equipped_item_any_tag('slot.armor.legs', 'mutant_powers') && !q.equipped_item_any_tag('slot.armor.legs', 'black_panther_powers') && !q.equipped_item_any_tag('slot.armor.legs', 'white_tiger_amulet') && !q.equipped_item_any_tag('slot.armor.legs', 'spiderman_powers');",
		  "variable.dr_octopus_take = q.mark_variant == 32;",
		  //ability0 == 4 is off_walk
		  "variable.off_walk = q.property('arathnido:ability0') == 4 || variable.off_walk_on_fly || ((q.equipped_item_any_tag('slot.armor.chest', 'zombie_dr_strange_capa') || q.equipped_item_any_tag('slot.armor.feet', 'fly_system') || q.equipped_item_any_tag('slot.armor.legs', 'fly_system')) && !q.is_on_ground);",
		  "variable.off_sneaking = q.equipped_item_any_tag('slot.armor.legs', 'off_sneaking') || q.equipped_item_any_tag('slot.armor.feet', 'off_sneaking') || q.equipped_item_any_tag('slot.armor.head', 'off_sneaking') || q.equipped_item_any_tag('slot.armor.chest', 'off_sneaking') || q.equipped_item_any_tag('slot.armor.chest', 'dr_octopus_arms') || (q.equipped_item_any_tag('slot.armor.chest', 'zombie_dr_strange_capa') && !q.is_on_ground) || (q.equipped_item_any_tag('slot.weapon.mainhand', 'mjolnir') && !q.is_on_ground);",  
          "variable.attack = Math.sin(variable.attack_time * 38.17);",
          "variable.helmet_layer_visible = !query.has_head_gear;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
          "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.first_person_rotation_factor = math.sin((1 - variable.attack_time) * 180.0);",
          "variable.hand_bob = query.life_time < 0.01 ? 0.0 : variable.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;",
          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0.0, 1.0);",
          "variable.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;"
